{
  "version": "1.0",
  "mediawiki": {
    "name": "OpenRefine KB Test Wikibase",
    "root": "http://www.localhost:81/wiki/",
    "main_page": "http://www.localhost:81/wiki/Main_Page",
    "api": "http://www.localhost:81/w/api.php"
  },
  "wikibase": {
    "site_iri": "http://wikibase/entity/",
    "maxlag": 5,
    "max_edits_per_minute": 60,
    "tag": "openrefine-${version}",
    "properties": {
      "instance_of": "P1",
      "subclass_of": "P2"
    }
  },
  "oauth": {
    "registration_page": "http://www.localhost:81/wiki/Special:OAuthConsumerRegistration/propose"
  },
  "reconciliation": {
    "endpoint": "http://www.localhost:8000/${lang}/api"
  },
  "entity_types": {
    "item": {
      "site_iri": "http://wikibase/entity/",
      "reconciliation_endpoint": "http://www.localhost:8000/${lang}/api",
      "mediawiki_api": "http://www.localhost:81/w/api.php"
    }
  }
}
